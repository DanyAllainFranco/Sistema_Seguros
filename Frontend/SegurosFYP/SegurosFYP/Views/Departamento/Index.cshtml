@model IEnumerable<SegurosFYP.Models.DepartamentoViewModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Departamentos</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table table-responsive table-bordered">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Depar_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Depar_Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Depar_UsuarioCreacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Depar_FechaCreacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Depar_UsuarioModificacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Depar_FechaModificacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Depar_Estado)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Depar_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Depar_Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Depar_UsuarioCreacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Depar_FechaCreacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Depar_UsuarioModificacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Depar_FechaModificacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Depar_Estado)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
}
    </tbody>
</table>
