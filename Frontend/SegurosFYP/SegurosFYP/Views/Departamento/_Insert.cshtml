@model SegurosFYP.Models.DepartamentoViewModel
<hr />
<div class="row">
    <div class="col-md-12">
        <form asp-action="Insert">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label asp-for="Depar_Id" class="control-label"></label>
                        <input asp-for="Depar_Id" class="form-control" />
                        <span asp-validation-for="Depar_Id" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label asp-for="Depar_Descripcion" class="control-label"></label>
                        <input asp-for="Depar_Descripcion" class="form-control" />
                        <span asp-validation-for="Depar_Descripcion" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
