@model SegurosFYP.Models.ParentescoViewModel

        @{
        ViewData["Title"] = "_Insert";
        ViewData["ModelName"] = "ParentescoViewModel".Replace("ViewModel","");
        }
        
<form asp-action="Insert">
    <div class="d-flex justify-content-center flex-wrap">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group col-sm-5">
            <label asp-for="Paren_Descripcion" class="control-label"></label>
            <input asp-for="Paren_Descripcion" class="form-control" />
            <span asp-validation-for="Paren_Descripcion" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group">
        <input type="submit" value="Guardar" class="btn btn-primary" />
        <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
    </div>
</form>
                @section Scripts {
                @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
                }



